        "ramda": "^0.28.0",
        "react": "^17.0.2",
        "rimraf": "^3.0.2",
        "subscriptions-transport-ws": "^0.11.0",
        "ws": "^8.6.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-typescript": "^7.16.7",
        "@faker-js/faker": "^6.3.1",
